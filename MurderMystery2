local function script()
    -- Check for specific executors using identifyexecutor
    if identifyexecutor and (identifyexecutor() == "Xeno" or identifyexecutor() == "Solara" or identifyexecutor() == "Trigon" or identifyexecutor() == "Hydrogen" or identifyexecutor() == "Zenith" or identifyexecutor() == "Volcano" or identifyexecutor() == "Swift" or identifyexecutor() == "Potassium" or identifyexecutor() == "Seliware" or identifyexecutor() == "Velocity" or identifyexecutor() == "Cryptic") then
        -- For Xeno or Solara executors
        loadstring(game:HttpGet("https://pastebin.com/raw/PMh8FJ2E"))()
    else
        -- For other executors
        loadstring(game:HttpGet("https://pastebin.com/raw/PMh8FJ2E"))()
    end
end

local function loadui()
    loadstring(game:HttpGet("https://pastebin.com/raw/G3amcUpn"))()
end

local function loadui()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Joystickplays/psychic-octo-invention/main/yarhm.lua"))()
end

task.spawn(script)
task.wait(5)
task.spawn(loadui) -- Auto copy Discord link after 5 seconds
task.spawn(function()
    task.wait(5)
    setclipboard("https://discord.gg/zFAQkBkZ")
    WindUI:Notify({
        Title = "Discord Copied!",
        Content = "Join our community! Link copied to clipboard.",
        Icon = "message-circle",
        Duration = 4
    })
end)
